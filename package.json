{
  "name": "react-native-nitro-event-kit",
  "version": "1.7.6",
  "description": "react-native-nitro-event-kit",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "react-native.config.js",
    "lib",
    "nitrogen",
    "android/build.gradle",
    "android/gradle.properties",
    "android/CMakeLists.txt",
    "android/src",
    "ios/**/*.h",
    "ios/**/*.m",
    "ios/**/*.mm",
    "ios/**/*.cpp",
    "ios/**/*.swift",
    "app.plugin.js",
    "*.podspec",
    "README.md"
  ],
  "scripts": {
    "codegen": "rm -rf node_modules nitrogen lib && yarn install && yarn build && npx nitro-codegen && cd example && rm -rf node_modules && yarn install && npx pod-install",
    "build": "bob build",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf android/build node_modules/**/android/build lib",
    "lint-ci": "eslint \"**/*.{js,ts,tsx}\" -f @jamesacarr/github-actions",
    "typescript": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "husky": "husky",
    "specs": "typescript && nitro-codegen --logLevel=\"debug\""
  },
  "keywords": [
    "react-native",
    "nitro",
    "react-native-nitro-event-kit",
    "react-native-calendar"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VladyslavMartynov10/react-native-nitro-event-kit.git"
  },
  "author": "Vlad Martynov <me@vladyslav.martynov.10@gmail.com> (https://github.com/VladyslavMartynov10)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VladyslavMartynov10/react-native-nitro-event-kit/issues"
  },
  "homepage": "https://github.com/VladyslavMartynov10/react-native-nitro-event-kit",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@react-native/babel-preset": "0.81.0",
    "@react-native/eslint-config": "0.81.0",
    "@release-it/conventional-changelog": "^10.0.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^19.0.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "nitro-codegen": "*",
    "prettier": "^3.4.2",
    "react": "19.1.0",
    "react-native": "0.81.0",
    "react-native-builder-bob": "^0.36.0",
    "react-native-nitro-modules": "*",
    "release-it": "^18.1.2",
    "typescript": "^5.5.4"
  },
  "release-it": {
    "git": {
      "commitMessage": "feat: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-nitro-modules": "*"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "semi": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.json"
        }
      ]
    ]
  },
  "packageManager": "yarn@4.7.0+sha512.5a0afa1d4c1d844b3447ee3319633797bcd6385d9a44be07993ae52ff4facabccafb4af5dcd1c2f9a94ac113e5e9ff56f6130431905884414229e284e37bb7c9"
}
